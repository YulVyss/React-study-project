.preventDefault()\r\n        let url = event.target.elements.url.value\r\n        url = url.trim()\r\n        if (url === '') {\r\n            alert('Заполните поля')\r\n            return false\r\n        }\r\n        noteURL = url\r\n        window.location.href = env.url + '/' + url\r\n\r\n    }\r\n    const searchNote = () => {\r\n        window.location.href = env.url\r\n    }\r\n\r\n    return (\r\n        <div className=\"container\">\r\n            <h1>Note search</h1>\r\n            <div className={lineClass}>\r\n                <div class=\"card-body\">\r\n                    <h5 className=\"card-title\">Note: </h5>\r\n                    <p className=\"card-text\">{noteText}</p>\r\n                </div>\r\n\r\n                <p><button onClick={searchNote} className=\"btn btn-secondary\">Смотреть еще один note</button></p>\r\n            </div>\r\n            <div className={errorClass}>\r\n                <p className=\"alert alert-danger\" role=\"alert\">Произошла ошибка. Такой note не найден!!</p>\r\n                <p><button onClick={searchNote} className=\"btn btn-secondary\">Смотреть еще один note</button></p>\r\n            </div>\r\n            <div className={formClass}>\r\n                <form action=\"\" onSubmit={getNote}>\r\n                    <div className=\"input-group mb-3\">\r\n                        <label htmlFor=\"url\" className=\"input-group-text\">Введите hash заметки</label>\r\n                        <input type=\"text\" name=\"url\" id=\"url\" className=\"form-control\" />\r\n                        <button type=\"submit\" className=\"btn btn-primary\">Искать Note</button>\r\n                    </div>\r\n\r\n                </form>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\vysso\\Desktop\\web-development\\Study\\ReactLushchenko\\Project\\react_project\\src\\components\\Error.js",[],{"ruleId":"44","severity":1,"message":"45","line":40,"column":8,"nodeType":"46","endLine":40,"endColumn":10,"suggestions":"47"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'noteURL'. Either include it or remove the dependency array.","ArrayExpression",["48"],{"desc":"49","fix":"50"},"Update the dependencies array to be: [noteURL]",{"range":"51","text":"52"},[1512,1514],"[noteURL]"]